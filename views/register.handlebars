<div class="container">
        <div class="row">
            <div class="col align-self-center">
            <form>
                <div class="form-group">
                    <label for="inputEmail1">Email address</label>
                    <input type="text" class="form-control" id="emailOrUsername_form" aria-describedby="emailHelp" placeholder="Enter email or username">
                    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
                </div>

                <div class="form-group">
                    <label for="inputUser">User Name</label>
                    <input type="text" class="form-control" id="username_form" aria-describedby="emailHelp" placeholder="Enter username">
                    
                </div>




                <div class="form-group">
                    <label for="inputPassword1">Password</label>
                    <input type="text" class="form-control" id="password_form" placeholder="Password">
                </div>
                <div class="form-group">
                    <label for="inputPassword2">Re-enter password</label>
                    <input type="text" class="form-control" id="password_match" placeholder="Re-enter password">
                </div>
            </form>
            </div>
        </div>
        <div class="row">
            <button type="button" class="btn btn-primary btn-lg btn-block" id="submit_btn">Submit</button>
        </div>
    </div>
